@model List<Competitor>

@{

	ViewBag.Title = "SingaporeSkills 2018";

}

<h2>SingaporeSkills 2018 Competitors</h2>

<span style="color:red">@TempData["Msg"]</span><br />

<p>

	<a asp-controller="Competitor" asp-action="Create">Create New</a>

</p>

<table class="table">

	<tr>

		<th>Id</th>

		<th>Name</th>

		<th>Actions</th>

	</tr>



	@foreach (var item in Model)

	{

		<tr>

			<td>

				@item.Id

			</td>

			<td>

				@item.Name

			</td>

			<td>

				<a asp-action="Update" asp-route-id="@item.Id">Update</a>

			</td>

		</tr>

	}

</table>

